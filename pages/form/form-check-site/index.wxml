<!--index.wxml-->
<view class="page">
  <wxc-notice 
    scroll="{{true}}" 
    close="{{true}}" 
    bg-color="#ff9300" 
    bg-rgba="rgba(255, 147, 0, 0)" 
    show-icon="{{true}}" 
    notice="{{notice}}" 
    class="wxc-notice">
  </wxc-notice>
  <view class="list-item">
    <map
       class="map" 
       longitude="{{longitude}}" 
       latitude=" {{latitude}}" 
       markers="{{markers}}" 
       scale="13" 
       show-location 
       style="width: 100%; height: 100%;">
    </map>
  </view>
</view>
